package com.tcs.sample.Threads;

public class SampleThread extends Thread{
	private String msg;
	public SampleThread(String msg) {
		this.msg=msg;
	}
	public void run() {
		for (int i=0;i<=10;i++) {
			System.out.println(msg);
			try {
				Thread.sleep(300);
			}catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}

}
